            Variant: verticalAutoLayoutContainer
            Properties:
              DropShadow: =DropShadow.None
              LayoutDirection: =LayoutDirection.Vertical
              LayoutJustifyContent: =LayoutJustifyContent.Center
              LayoutMinHeight: =10
              LayoutMode: =LayoutMode.Auto
              RadiusBottomLeft: =0
              RadiusBottomRight: =0
              RadiusTopLeft: =0
              RadiusTopRight: =0
            Children:
            - item_title_label:
                Control: Label
                Properties:
                  Text: ="Title"
                  AutoHeight: =true
                  BorderColor: =RGBA(0, 0, 0, 0)
                  Color: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.FONT.COLOR
                  DisabledBorderColor: =Self.BorderColor
                  DisabledColor: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.DISABLED_COLOR
                  DisabledFill: =Color.Transparent
                  Font: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.FONT.TYPE
                  FontWeight: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.FONT.FONT_WEIGHT.TITLE
                  LayoutMinWidth: =10
                  PaddingBottom: =0
                  PaddingLeft: =0
                  PaddingRight: =0
                  PaddingTop: =0
                  Size: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.FONT.SIZE_DEFAULT
            - item_subtitle_label:
                Control: Label
                Properties:
                  Text: ="Subtitle"
                  AutoHeight: =true
                  BorderColor: =RGBA(0, 0, 0, 0)
                  Color: =LookUp(THEMING,ID = THEME_MODE_ID).VALUES.FONT.COLOR
